{
  "es": {
    "timefield": "received_at",
    "default_index": "logstash-*",
    "allow_url_parameter": false,
    "timezone": "UTC"
  },
  "tail_interval_in_seconds": 10,
  "max_buckets": 500,
  "fields" : [
    {
      "type" : "log",
      "fields": ["@timestamp", "Environment", "Level", "Message"]
    }
  ]
}
